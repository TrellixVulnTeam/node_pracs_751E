	"keywords": [],
	"description": "",
	"dependencies": {
		"fastify": "^3.29.0",
		"fastify-swagger": "^5.2.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"pino-pretty": "^7.6.1",
