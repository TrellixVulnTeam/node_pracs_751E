		"dev": "nodemon src/server.js",
		"test": "tap --reporter=list --watch",
		"format": "prettier --write \"src/**/*.js\" \"src/*.js\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.js\" --fix",
		"serve": "nohup node src/server.js"
	},
